{
  "name": "paperscript",
  "displayName": "PaperScript",
  "description": "Syntax highlighting for PaperScript",
  "version": "0.0.1",
  "publisher": "BluCarbon Labs s.r.o.",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./extension.js",
  "contributes": {
    "languages": [
      {
        "id": "paperscript",
        "aliases": [
          "PaperScript",
          "paperscript"
        ],
        "extensions": [
          ".pps"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "paperscript",
        "scopeName": "source.paperscript",
        "path": "./syntaxes/paperscript.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/vscode": "^1.100.0",
    "vscode": "^1.1.37"
  }
}
